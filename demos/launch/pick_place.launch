<launch>
    <include file="$(find turtlebot3_manipulation_gazebo)/launch/turtlebot3_manipulation_gazebo.launch" >
        <env name="TURTLEBOT3_MODEL" value="waffle" />
    </include>

    <include file="$(find turtlebot3_manipulation_moveit_config)/launch/move_group.launch">
        <env name="TURTLEBOT3_MODEL" value="waffle" />
    </include>

    <include file="$(find open_manipulator_controller)/launch/open_manipulator_controller.launch">
        <arg name="use_platform" value="false" />
    </include>

    <node pkg="demos" type="pick_and_place" name="pick_and_place" output="screen"></node>
</launch>